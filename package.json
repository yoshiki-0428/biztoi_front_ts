{
  "name": "biztoi_front_ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:update": "yarn openapi:update && yarn openapi && vue-cli-service serve --port 3000 --mode dev",
    "serve:sync": "yarn openapi:sync && yarn openapi && vue-cli-service serve --port 3000 --mode dev",
    "serve:heroku": "yarn openapi:sync && yarn openapi && vue-cli-service serve --port 3000 --mode heroku",
    "serve:https": "yarn openapi && vue-cli-service serve --port 3000 --mode dev --https",
    "build": "yarn openapi && vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "openapi": "yarn openapi:biztoi && yarn openapi:books-api",
    "openapi:sync": "git submodule update --init",
    "openapi:update": "git submodule update --remote",
    "openapi:biztoi": "openapi-generator generate -i openapi/reference/biztoi/openapi.yaml -g typescript-axios -o src/axios/biztoi",
    "openapi:books-api": "openapi-generator generate -i openapi/reference/rakuten/books.yaml -g typescript-axios -o src/axios/books --skip-validate-spec "
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-analytics": "^5.22.1",
    "vue-class-component": "^7.0.2",
    "vue-hotjar": "^1.1.1",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.9",
    "vuex": "^3.0.1",
    "vuex-connect": "^2.2.0",
    "vuex-module-decorators": "^0.11.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@mdi/font": "^4.5.95",
    "@openapitools/openapi-generator-cli": "^1.0.1-4.1.3",
    "@types/chai": "^4.1.0",
    "@types/lodash": "^4.14.146",
    "@types/mocha": "^5.2.4",
    "@types/uuid": "^3.4.6",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "^4.1.2",
    "deepmerge": "^4.2.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.2",
    "lint-staged": "^9.4.2",
    "material-design-icons-iconfont": "^5.0.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "ts-clipboard": "^1.0.17",
    "typescript": "~3.5.3",
    "uuid": "^3.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
